        <Label Content="Перестановка:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="215,207,0,0" Grid.Column="1" Height="26" Width="90"/>
        <TextBox x:Name="Substitute2TB" HorizontalAlignment="Left" Height="23" Margin="311,211,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="450" Grid.Column="1"/>
