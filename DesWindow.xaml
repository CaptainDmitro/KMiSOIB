<Window x:Class="KMiSOIB.DesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KMiSOIB"
        mc:Ignorable="d"
        Title="DesWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="MessageTB" HorizontalAlignment="Left" Height="23" Margin="59,19,0,0" TextWrapping="Wrap" Text="ДРУЖКОВ " VerticalAlignment="Top" Width="285"/>
        <TextBox x:Name="KeyTB" HorizontalAlignment="Left" Height="23" Margin="59,60,0,0" TextWrapping="Wrap" Text="ДИМОЧК" VerticalAlignment="Top" Width="285"/>
        <Button x:Name="Proceed_Button" Content="Button" HorizontalAlignment="Left" Margin="128,292,0,0" VerticalAlignment="Top" Width="75" Click="Proceed_Button_Click"/>
        <TextBox x:Name="xorTB" HorizontalAlignment="Left" Height="23" Margin="59,95,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="285"/>

    </Grid>
</Window>
